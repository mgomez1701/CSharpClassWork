
@{
    ViewData["Title"] = "ChuckJoke";
}

@*we only are getting one response back so we do not nee a list*@

@model Chuck 
<h1>Chuck Joke</h1>

@Model.value.joke

<form action="../Chuck/UserNameJoke" method="post">
    <input type="text" name="FirstName" placeholder="First Name" />
    <input type="text" name="LastName" placeholder="Last Name" />

    <input type="submit" class="btn btn-primary" value="What's your joke?" />



</form>



